{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },
    "extensions": {
        "http": {
            "customHeaders": {
                "access-control-allow-origin": "*",
                "access-control-allow-headers": "*",
                "access-control-max-age": 3600,
                "access-control-allow-methods": "post, get, delete, patch"
            }
        }
    }
}